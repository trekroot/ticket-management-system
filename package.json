{
  "name": "ticket-management-system",
  "version": "0.8.1-alpha",
  "description": "Ticket management system for Hearts of Pine",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev-seed-data": "npm run seed:all && nodemon server.js",
    "seed:all": "npm run seed:users && npm run seed:games && npm run seed:tickets",
    "seed:users": "node src/scripts/seedUsers.js",
    "seed:games": "node src/scripts/seedGames.js",
    "seed:tickets": "node src/scripts/seedTickets.js",
    "precommit": "npm run lint && npm audit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "tickets",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase-admin": "^13.6.0",
    "mongoose": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "nodemon": "^3.1.11"
  }
}
